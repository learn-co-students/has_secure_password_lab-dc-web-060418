<h1>Create new user</h1>
<%= form_for :user, url: '/signup' do |f| %>
<label for="">Name:</label>
<%= f.text_field :name %>
<label for="">Password:</label>
<%= f.password_field :password %>
<label for="">Confirm Password:</label>
<%= f.password_field :password_confirmation %>
<%= f.submit %>
<% end %>